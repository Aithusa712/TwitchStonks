{
  "routes": [
    {
      "route": "/ws",
      "allowedRoles": ["anonymous"]
    },
        {
      "route": "/ws/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/ws", "/ws/*", "/api/*", "/*.{js,css,png,jpg,ico,svg}"]  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "networking": {
    "allowedIpRanges": []
  }
}
